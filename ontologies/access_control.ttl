@prefix : <http://ju.se/Ontology-BasedAccessControlModule#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix swrl: <http://www.w3.org/2003/11/swrl#> .
@prefix OBACM: <http://ju.se/Ontology-BasedAccessControlModule#> .
@prefix swrla: <http://swrl.stanford.edu/ontologies/3.3/swrla.owl#> .
@prefix swrlb: <http://www.w3.org/2003/11/swrlb#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix opla-sd: <http://ontologydesignpatterns.org/opla-sd#> .
@base <http://ju.se/Ontology-BasedAccessControlModule#> .

<http://ju.se/Ontology-BasedAccessControlModule#> rdf:type owl:Ontology ;
                                                   owl:versionIRI :v1 ;
                                                   owl:imports odrl: ,
                                                               <http://www.w3.org/ns/prov#> ;
                                                   dcterms:creator "Bincy Veena" ,
                                                                   "Vineetha Shajan" ;
                                                   dcterms:contributor "Rahel Kebede" ,
                                                                        "Peter Johansson" ,
                                                                        "Annika Moscati" ;
                                                   dcterms:created "2025-05-15"^^xsd:date ;
                                                   dcterms:description "This ontology is a semantic model that enables dynamic, role- and context-based access control for Digital Product Passports within Built environment." ;
                                                   dcterms:license <https://creativecommons.org/licenses/by/4.0/> ;
                                                   dcterms:title "Digital Product Passport â€“ Access Control Ontology Module" ;
                                                   owl:versionInfo "1.0" .

#################################################################
#    Annotation properties
#################################################################

###  http://ontologydesignpatterns.org/opla-sd#entityPosition
opla-sd:entityPosition rdf:type owl:AnnotationProperty .


###  http://ontologydesignpatterns.org/opla-sd#entityPositionX
opla-sd:entityPositionX rdf:type owl:AnnotationProperty .


###  http://ontologydesignpatterns.org/opla-sd#entityPositionY
opla-sd:entityPositionY rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
swrla:isRuleEnabled rdf:type owl:AnnotationProperty .


###  http://www.w3.org/ns/odrl/2/function
odrl:function rdf:type owl:AnnotationProperty .


###  http://www.w3.org/ns/odrl/2/relation
odrl:relation rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://ju.se/Ontology-BasedAccessControlModule#assigneeRole
OBACM:assigneeRole rdf:type owl:ObjectProperty ,
                            owl:FunctionalProperty ,
                            owl:AsymmetricProperty ,
                            owl:IrreflexiveProperty ;
                   rdfs:domain odrl:Policy ;
                   rdfs:range OBACM:UserRole ;
                   rdfs:comment "Specifies the role held by the assignee in the context of the policy, indicating their function or responsibility with respect to the granted permissions." .


###  http://ju.se/Ontology-BasedAccessControlModule#generatedDecision
OBACM:generatedDecision rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf <http://www.w3.org/ns/prov#wasGeneratedBy> ,
                                           <http://www.w3.org/ns/prov#wasInfluencedBy> ;
                        rdfs:domain OBACM:AccessDecisionEntity ,
                                    <http://www.w3.org/ns/prov#Entity> ,
                                    [ rdf:type owl:Class ;
                                      owl:unionOf ( <http://www.w3.org/ns/prov#Activity>
                                                    <http://www.w3.org/ns/prov#Agent>
                                                    <http://www.w3.org/ns/prov#Entity>
                                                  )
                                    ] ;
                        rdfs:range OBACM:UserRequestAction ,
                                   <http://www.w3.org/ns/prov#Activity> ,
                                   [ rdf:type owl:Class ;
                                     owl:unionOf ( <http://www.w3.org/ns/prov#Activity>
                                                   <http://www.w3.org/ns/prov#Agent>
                                                   <http://www.w3.org/ns/prov#Entity>
                                                 )
                                   ] ;
                        rdfs:comment "Links a decision outcome to the access activity that produced it." .


###  http://ju.se/Ontology-BasedAccessControlModule#hasCondition
OBACM:hasCondition rdf:type owl:ObjectProperty ,
                            owl:AsymmetricProperty ,
                            owl:IrreflexiveProperty ;
                   rdfs:domain odrl:Policy ;
                   rdfs:range OBACM:Context ;
                   rdfs:comment "Links a policy or rule to a condition that must be satisfied for the associated permissions or constraints to apply." .


###  http://ju.se/Ontology-BasedAccessControlModule#hasContext
OBACM:hasContext rdf:type owl:ObjectProperty ,
                          owl:AsymmetricProperty ,
                          owl:IrreflexiveProperty ;
                 rdfs:domain odrl:Party ;
                 rdfs:range OBACM:Context ;
                 rdfs:comment "Links a user to contextual information that may influence access control decisions." .


###  http://ju.se/Ontology-BasedAccessControlModule#hasRole
OBACM:hasRole rdf:type owl:ObjectProperty ,
                       owl:FunctionalProperty ,
                       owl:AsymmetricProperty ,
                       owl:IrreflexiveProperty ;
              rdfs:domain OBACM:User ,
                          odrl:Party ,
                          <http://www.w3.org/ns/prov#Agent> ,
                          [ rdf:type owl:Class ;
                            owl:unionOf ( <http://www.w3.org/ns/prov#Activity>
                                          <http://www.w3.org/ns/prov#Agent>
                                          <http://www.w3.org/ns/prov#Entity>
                                        )
                          ] ;
              rdfs:range OBACM:UserRole ;
              rdfs:comment "Associates a user with a specific role within the system for access control purposes." .


###  http://ju.se/Ontology-BasedAccessControlModule#requestedAction
OBACM:requestedAction rdf:type owl:ObjectProperty ,
                               owl:FunctionalProperty ,
                               owl:AsymmetricProperty ,
                               owl:IrreflexiveProperty ;
                      rdfs:domain OBACM:UserRequestAction ,
                                  <http://www.w3.org/ns/prov#Activity> ,
                                  [ rdf:type owl:Class ;
                                    owl:unionOf ( <http://www.w3.org/ns/prov#Activity>
                                                  <http://www.w3.org/ns/prov#Agent>
                                                  <http://www.w3.org/ns/prov#Entity>
                                                )
                                  ] ;
                      rdfs:range odrl:Action ;
                      rdfs:comment "Links a request to the specific action being requested by the user." .


###  http://ju.se/Ontology-BasedAccessControlModule#requestedAsset
OBACM:requestedAsset rdf:type owl:ObjectProperty ,
                              owl:FunctionalProperty ,
                              owl:AsymmetricProperty ,
                              owl:IrreflexiveProperty ;
                     rdfs:domain OBACM:UserRequestAction ,
                                 <http://www.w3.org/ns/prov#Activity> ,
                                 [ rdf:type owl:Class ;
                                   owl:unionOf ( <http://www.w3.org/ns/prov#Activity>
                                                 <http://www.w3.org/ns/prov#Agent>
                                                 <http://www.w3.org/ns/prov#Entity>
                                               )
                                 ] ;
                     rdfs:range OBACM:DPPModules ,
                                odrl:Asset ;
                     rdfs:comment "Connects a request to the asset that the user wants to access or manipulate." .


###  http://ju.se/Ontology-BasedAccessControlModule#requestedBy
OBACM:requestedBy rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf <http://www.w3.org/ns/prov#wasAssociatedWith> ,
                                     <http://www.w3.org/ns/prov#wasInfluencedBy> ;
                  rdf:type owl:FunctionalProperty ,
                           owl:AsymmetricProperty ,
                           owl:IrreflexiveProperty ;
                  rdfs:domain OBACM:UserRequestAction ,
                              <http://www.w3.org/ns/prov#Activity> ,
                              [ rdf:type owl:Class ;
                                owl:unionOf ( <http://www.w3.org/ns/prov#Activity>
                                              <http://www.w3.org/ns/prov#Agent>
                                              <http://www.w3.org/ns/prov#Entity>
                                            )
                              ] ;
                  rdfs:range OBACM:User ,
                             odrl:Party ,
                             <http://www.w3.org/ns/prov#Agent> ,
                             [ rdf:type owl:Class ;
                               owl:unionOf ( <http://www.w3.org/ns/prov#Activity>
                                             <http://www.w3.org/ns/prov#Agent>
                                             <http://www.w3.org/ns/prov#Entity>
                                           )
                             ] ;
                  rdfs:comment "Indicates who performed the access request activity." .


###  http://www.w3.org/ns/odrl/2/action
odrl:action rdf:type owl:ObjectProperty ,
                     owl:FunctionalProperty ,
                     owl:AsymmetricProperty ,
                     owl:IrreflexiveProperty ;
            rdfs:domain odrl:Permission ;
            rdfs:range odrl:Action ;
            rdfs:comment "An object property that links a rule (e.g., a Permission or Prohibition) to the action that is being regulated, such as 'use', 'delete', or 'transfer'.\"" .


###  http://www.w3.org/ns/odrl/2/assignee
odrl:assignee rdf:type owl:ObjectProperty ,
                       owl:IrreflexiveProperty ;
              rdfs:domain odrl:Policy ;
              rdfs:range OBACM:User ,
                         odrl:Party ,
                         <http://www.w3.org/ns/prov#Agent> ,
                         [ rdf:type owl:Class ;
                           owl:unionOf ( <http://www.w3.org/ns/prov#Activity>
                                         <http://www.w3.org/ns/prov#Agent>
                                         <http://www.w3.org/ns/prov#Entity>
                                       )
                         ] ;
              rdfs:comment "Relates a Policy to the User or agent that is granted the right to perform the specified actions defined in the policy." .


###  http://www.w3.org/ns/odrl/2/assigner
odrl:assigner rdf:type owl:ObjectProperty ,
                       owl:FunctionalProperty ,
                       owl:AsymmetricProperty ,
                       owl:IrreflexiveProperty ;
              rdfs:domain odrl:Policy .


###  http://www.w3.org/ns/odrl/2/hasPolicy
odrl:hasPolicy rdf:type owl:ObjectProperty ,
                        owl:AsymmetricProperty ,
                        owl:IrreflexiveProperty ;
               rdfs:domain odrl:Permission ;
               rdfs:range odrl:Policy .


###  http://www.w3.org/ns/odrl/2/permission
odrl:permission rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ,
                         owl:AsymmetricProperty ,
                         owl:IrreflexiveProperty ;
                rdfs:domain odrl:Policy ;
                rdfs:range odrl:Permission ;
                rdfs:comment "Specifies the permissions granted by a policy, typically linking to an ODRL Permission instance that defines allowed actions." .


###  http://www.w3.org/ns/odrl/2/prohibition
odrl:prohibition rdf:type owl:ObjectProperty ,
                          owl:AsymmetricProperty ,
                          owl:IrreflexiveProperty .


###  http://www.w3.org/ns/odrl/2/target
odrl:target rdf:type owl:ObjectProperty ,
                     owl:AsymmetricProperty ,
                     owl:IrreflexiveProperty ;
            rdfs:domain odrl:Permission ;
            rdfs:range OBACM:DPPModules ,
                       odrl:Asset ;
            rdfs:comment "Specifies the asset that a rule (permission, prohibition, or obligation) applies to." .


###  http://www.w3.org/ns/prov#used
<http://www.w3.org/ns/prov#used> rdfs:domain [ rdf:type owl:Class ;
                                               owl:unionOf ( <http://www.w3.org/ns/prov#Activity>
                                                             <http://www.w3.org/ns/prov#Agent>
                                                             <http://www.w3.org/ns/prov#Entity>
                                                           )
                                             ] ;
                                 rdfs:range [ rdf:type owl:Class ;
                                              owl:unionOf ( <http://www.w3.org/ns/prov#Activity>
                                                            <http://www.w3.org/ns/prov#Agent>
                                                            <http://www.w3.org/ns/prov#Entity>
                                                          )
                                            ] .


###  http://www.w3.org/ns/prov#wasAssociatedWith
<http://www.w3.org/ns/prov#wasAssociatedWith> rdfs:domain [ rdf:type owl:Class ;
                                                            owl:unionOf ( <http://www.w3.org/ns/prov#Activity>
                                                                          <http://www.w3.org/ns/prov#Agent>
                                                                          <http://www.w3.org/ns/prov#Entity>
                                                                        )
                                                          ] ;
                                              rdfs:range [ rdf:type owl:Class ;
                                                           owl:unionOf ( <http://www.w3.org/ns/prov#Activity>
                                                                         <http://www.w3.org/ns/prov#Agent>
                                                                         <http://www.w3.org/ns/prov#Entity>
                                                                       )
                                                         ] .


###  http://www.w3.org/ns/prov#wasGeneratedBy
<http://www.w3.org/ns/prov#wasGeneratedBy> rdfs:domain [ rdf:type owl:Class ;
                                                         owl:unionOf ( <http://www.w3.org/ns/prov#Activity>
                                                                       <http://www.w3.org/ns/prov#Agent>
                                                                       <http://www.w3.org/ns/prov#Entity>
                                                                     )
                                                       ] ;
                                           rdfs:range [ rdf:type owl:Class ;
                                                        owl:unionOf ( <http://www.w3.org/ns/prov#Activity>
                                                                      <http://www.w3.org/ns/prov#Agent>
                                                                      <http://www.w3.org/ns/prov#Entity>
                                                                    )
                                                      ] .


#################################################################
#    Data properties
#################################################################

###  http://ju.se/Ontology-BasedAccessControlModule#isPermitted
OBACM:isPermitted rdf:type owl:DatatypeProperty ,
                           owl:FunctionalProperty ;
                  rdfs:domain OBACM:UserRequestAction ,
                              <http://www.w3.org/ns/prov#Activity> ,
                              [ rdf:type owl:Class ;
                                owl:unionOf ( <http://www.w3.org/ns/prov#Activity>
                                              <http://www.w3.org/ns/prov#Agent>
                                              <http://www.w3.org/ns/prov#Entity>
                                            )
                              ] ;
                  rdfs:range xsd:boolean ;
                  opla-sd:entityPosition [ opla-sd:entityPositionX "429.635084042631"^^xsd:double ;
                                           opla-sd:entityPositionY "39.720426655858034"^^xsd:double ;
                                           rdfs:comment "This is an entity positioning annotation generated by CoModIDE (https://comodide.com/). Removing this annotation will break rendering the CoModIDE schema diagram view."@en
                                         ] ;
                  rdfs:comment "Indicates whether a given access request is permitted according to the defined policies." ;
                  rdfs:label "isPermitted" .


###  http://www.w3.org/ns/prov#generatedAtTime
<http://www.w3.org/ns/prov#generatedAtTime> rdfs:domain [ rdf:type owl:Class ;
                                                          owl:unionOf ( <http://www.w3.org/ns/prov#Activity>
                                                                        <http://www.w3.org/ns/prov#Agent>
                                                                        <http://www.w3.org/ns/prov#Entity>
                                                                      )
                                                        ] .


###  http://www.w3.org/ns/prov#startedAtTime
<http://www.w3.org/ns/prov#startedAtTime> rdf:type owl:FunctionalProperty ;
                                          rdfs:domain [ rdf:type owl:Class ;
                                                        owl:unionOf ( <http://www.w3.org/ns/prov#Activity>
                                                                      <http://www.w3.org/ns/prov#Agent>
                                                                      <http://www.w3.org/ns/prov#Entity>
                                                                    )
                                                      ] .


###  http://www.w3.org/ns/prov#value
<http://www.w3.org/ns/prov#value> rdf:type owl:FunctionalProperty ;
                                  rdfs:domain [ rdf:type owl:Class ;
                                                owl:unionOf ( <http://www.w3.org/ns/prov#Activity>
                                                              <http://www.w3.org/ns/prov#Agent>
                                                              <http://www.w3.org/ns/prov#Entity>
                                                            )
                                              ] ;
                                  rdfs:range xsd:string .


#################################################################
#    Classes
#################################################################

###  http://ju.se/Ontology-BasedAccessControlModule#AccessControlPolicy
OBACM:AccessControlPolicy rdf:type owl:Class ;
                          rdfs:subClassOf odrl:Policy ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty odrl:permission ;
                                            owl:someValuesFrom odrl:Permission
                                          ] ;
                          opla-sd:entityPosition [ opla-sd:entityPositionX "600.2936540375185"^^xsd:double ;
                                                   opla-sd:entityPositionY "629.9998672076415"^^xsd:double ;
                                                   rdfs:comment "This is an entity positioning annotation generated by CoModIDE (https://comodide.com/). Removing this annotation will break rendering the CoModIDE schema diagram view."@en
                                                 ] ;
                          rdfs:comment "Policies tailored to DPPs conforming to ESPR" .


###  http://ju.se/Ontology-BasedAccessControlModule#AccessDecisionEntity
OBACM:AccessDecisionEntity rdf:type owl:Class ;
                           rdfs:subClassOf <http://www.w3.org/ns/prov#Entity> ,
                                           [ rdf:type owl:Class ;
                                             owl:unionOf ( <http://www.w3.org/ns/prov#Activity>
                                                           <http://www.w3.org/ns/prov#Agent>
                                                           <http://www.w3.org/ns/prov#Entity>
                                                         )
                                           ] ;
                           opla-sd:entityPosition [ opla-sd:entityPositionX "56.00845630731316"^^xsd:double ;
                                                    opla-sd:entityPositionY "130.11862303841394"^^xsd:double ;
                                                    rdfs:comment "This is an entity positioning annotation generated by CoModIDE (https://comodide.com/). Removing this annotation will break rendering the CoModIDE schema diagram view."@en
                                                  ] ;
                           rdfs:comment "Represents the outcome of evaluating an access request." .


###  http://ju.se/Ontology-BasedAccessControlModule#Context
OBACM:Context rdf:type owl:Class ;
              rdfs:subClassOf owl:Thing ;
              opla-sd:entityPosition [ opla-sd:entityPositionX "350.14704296319564"^^xsd:double ;
                                       opla-sd:entityPositionY "600.0443325899658"^^xsd:double ;
                                       rdfs:comment "This is an entity positioning annotation generated by CoModIDE (https://comodide.com/). Removing this annotation will break rendering the CoModIDE schema diagram view."@en
                                     ] ;
              rdfs:comment "Metadata associated with the users, such as, certification, affiliations etc." .


###  http://ju.se/Ontology-BasedAccessControlModule#DPPModules
OBACM:DPPModules rdf:type owl:Class ;
                 rdfs:subClassOf odrl:Asset ;
                 opla-sd:entityPosition [ opla-sd:entityPositionX "19.81731914528004"^^xsd:double ;
                                          opla-sd:entityPositionY "349.60156377860636"^^xsd:double ;
                                          rdfs:comment "This is an entity positioning annotation generated by CoModIDE (https://comodide.com/). Removing this annotation will break rendering the CoModIDE schema diagram view."@en
                                        ] ;
                 rdfs:comment "A collection of digital product passport (DPP) modules that contain structured information about the lifecycle, components, and provenance of a product." ;
                 rdfs:label "DPPModules" .


###  http://ju.se/Ontology-BasedAccessControlModule#User
OBACM:User rdf:type owl:Class ;
           rdfs:subClassOf odrl:Party ,
                           <http://www.w3.org/ns/prov#Agent> ,
                           [ rdf:type owl:Class ;
                             owl:unionOf ( <http://www.w3.org/ns/prov#Activity>
                                           <http://www.w3.org/ns/prov#Agent>
                                           <http://www.w3.org/ns/prov#Entity>
                                         )
                           ] ,
                           [ rdf:type owl:Class ;
                             owl:unionOf ( [ rdf:type owl:Restriction ;
                                             owl:onProperty OBACM:hasContext ;
                                             owl:someValuesFrom OBACM:Context
                                           ]
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty OBACM:hasRole ;
                                             owl:someValuesFrom OBACM:UserRole
                                           ]
                                         )
                           ] ;
           opla-sd:entityPosition [ opla-sd:entityPositionX "100.40000455473648"^^xsd:double ;
                                    opla-sd:entityPositionY "569.6218474339889"^^xsd:double ;
                                    rdfs:comment "This is an entity positioning annotation generated by CoModIDE (https://comodide.com/). Removing this annotation will break rendering the CoModIDE schema diagram view."@en
                                  ] ;
           rdfs:comment "User represents a person who requests an action." .


###  http://ju.se/Ontology-BasedAccessControlModule#UserRequestAction
OBACM:UserRequestAction rdf:type owl:Class ;
                        rdfs:subClassOf owl:Thing ,
                                        <http://www.w3.org/ns/prov#Activity> ,
                                        [ rdf:type owl:Class ;
                                          owl:unionOf ( <http://www.w3.org/ns/prov#Activity>
                                                        <http://www.w3.org/ns/prov#Agent>
                                                        <http://www.w3.org/ns/prov#Entity>
                                                      )
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty OBACM:requestedAsset ;
                                          owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                          owl:onClass OBACM:DPPModules
                                        ] ;
                        opla-sd:entityPosition [ opla-sd:entityPositionX "210.2135864813904"^^xsd:double ;
                                                 opla-sd:entityPositionY "99.79687630953471"^^xsd:double ;
                                                 rdfs:comment "This is an entity positioning annotation generated by CoModIDE (https://comodide.com/). Removing this annotation will break rendering the CoModIDE schema diagram view."@en
                                               ] ;
                        rdfs:comment "Represents the request made by teh user to access data within DPP, also a provenance activity" .


###  http://ju.se/Ontology-BasedAccessControlModule#UserRole
OBACM:UserRole rdf:type owl:Class ;
               rdfs:subClassOf owl:Thing ;
               owl:disjointWith odrl:Party ;
               opla-sd:entityPosition [ opla-sd:entityPositionX "239.7852667209255"^^xsd:double ;
                                        opla-sd:entityPositionY "640.3043222624462"^^xsd:double ;
                                        rdfs:comment "This is an entity positioning annotation generated by CoModIDE (https://comodide.com/). Removing this annotation will break rendering the CoModIDE schema diagram view."@en
                                      ] ;
               rdfs:comment "Represents a specific role assigned to a user within the OBACM system, determining their access rights and permitted actions over digital product passport data." .


###  http://www.w3.org/2002/07/owl#Thing
owl:Thing opla-sd:entityPosition [ opla-sd:entityPositionX "700.3292466020996"^^xsd:double ;
                                   opla-sd:entityPositionY "129.652153017148"^^xsd:double ;
                                   rdfs:comment "This is an entity positioning annotation generated by CoModIDE (https://comodide.com/). Removing this annotation will break rendering the CoModIDE schema diagram view."@en
                                 ] .


###  http://www.w3.org/ns/odrl/2/Action
odrl:Action rdf:type owl:Class ;
            rdfs:subClassOf owl:Thing ;
            opla-sd:entityPosition [ opla-sd:entityPositionX "210.3283092568812"^^xsd:double ;
                                     opla-sd:entityPositionY "290.3700932694753"^^xsd:double ;
                                     rdfs:comment "This is an entity positioning annotation generated by CoModIDE (https://comodide.com/). Removing this annotation will break rendering the CoModIDE schema diagram view."@en
                                   ] .


###  http://www.w3.org/ns/odrl/2/Asset
odrl:Asset rdf:type owl:Class ;
           rdfs:subClassOf owl:Thing ;
           opla-sd:entityPosition [ opla-sd:entityPositionX "19.977081972278256"^^xsd:double ;
                                    opla-sd:entityPositionY "169.67532082739524"^^xsd:double ;
                                    rdfs:comment "This is an entity positioning annotation generated by CoModIDE (https://comodide.com/). Removing this annotation will break rendering the CoModIDE schema diagram view."@en
                                  ] .


###  http://www.w3.org/ns/odrl/2/Party
odrl:Party rdf:type owl:Class ;
           rdfs:subClassOf owl:Thing ;
           opla-sd:entityPosition [ opla-sd:entityPositionX "49.6444780131979"^^xsd:double ;
                                    opla-sd:entityPositionY "449.99634777316237"^^xsd:double ;
                                    rdfs:comment "This is an entity positioning annotation generated by CoModIDE (https://comodide.com/). Removing this annotation will break rendering the CoModIDE schema diagram view."@en
                                  ] .


###  http://www.w3.org/ns/odrl/2/Permission
odrl:Permission rdf:type owl:Class ;
                rdfs:subClassOf owl:Thing ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty odrl:action ;
                                  owl:someValuesFrom odrl:Action
                                ] ;
                opla-sd:entityPosition [ opla-sd:entityPositionX "380.09698079370156"^^xsd:double ;
                                         opla-sd:entityPositionY "519.5956514275273"^^xsd:double ;
                                         rdfs:comment "This is an entity positioning annotation generated by CoModIDE (https://comodide.com/). Removing this annotation will break rendering the CoModIDE schema diagram view."@en
                                       ] .


###  http://www.w3.org/ns/odrl/2/Policy
odrl:Policy rdf:type owl:Class ;
            rdfs:subClassOf owl:Thing ;
            opla-sd:entityPosition [ opla-sd:entityPositionX "630.184896582791"^^xsd:double ;
